  <Shack>
    <Description>
      The Shack takes one food and produces one population.
    </Description>
    <Input>
      <Food>1</Food>
    </Input>
    <Output>
      <Population>1</Population>
    </Output>
    <Terrain>
      Land
    </Terrain>
  </Shack>
</Buildings>
