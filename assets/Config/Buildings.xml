<Buildings>
  <Farm>
    <Description>
      Farms always output 1 food per turn.
    </Description>
    <Input>
    </Input>
    <Output>
      <Food>1</Food>
    </Output>
    <Terrain>
      Land
    </Terrain>
  </Farm>

  <Barracks>
    <Description>
      The Barracks takes one population and outputs one Soldier.
    </Description>
    <Input>
      <Population>1</Population>
    </Input>
    <Output>
      <Soldier>1</Soldier>
    </Output>
    <Terrain>
      Land
    </Terrain>
